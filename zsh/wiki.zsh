#alias-wiki
alias-wiki () {
    echo "oh-my-alias-wiki"
    echo "curl.cc "
    echo "in : pacman -S"
    echo "un : pacman -Rns"
    echo "up : pacman -Syu"
    echo "pl : pacman -Qs"
    echo "pa : pacman -Ss"
    echo "pc : pacman -S"
    echo "po : pacman -Qtdq | sudo pacman -Rns "
    echo "sl : sway-launcher-desktop"
    echo "xy : watch -n 0.1 xdotool getmouselocation"
    echo "ys : hyprpicker"
    echo "e : emacs -nw"
    echo "v2 : sudo systemctl start v2raya.service"
    echo "ne : neofetch"
    echo "tl : tldr"
    echo "re : trash -put"
    echo ""
    
}

emacs-wiki (){
    bat ~/Y/N/wiki/emacs.org 
}

kitty-wiki (){
    bat ~/Y/N/wiki/kitty.org 
}

git-wiki (){
    bat ~/Y/N/wiki/Git 
}

pacman-wiki (){
    bat ~/Y/N/wiki/pacman 
}

ranger-wiki (){
    bat ~/Y/N/wiki/ranger 
}

